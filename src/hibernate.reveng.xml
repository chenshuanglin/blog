<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="myblog" match-name="blogclassify"/>
  <table-filter match-catalog="myblog" match-name="blogcontent"/>
  <table-filter match-catalog="myblog" match-name="comments"/>
  <table-filter match-catalog="myblog" match-name="users"/>
</hibernate-reverse-engineering>